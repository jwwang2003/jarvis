		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.41.1",
		"svelte-check": "^4.3.3",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.15",
