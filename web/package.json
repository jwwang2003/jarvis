	"engines": {
		"node": ">=18.0.0",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && gzip -c ./dist/favicon.svg > ./dist/favicon.svg.gz",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"postinstall": "node scripts/ensure-svelte-shims.cjs",
		"ts:check": "svelte-check --tsconfig ./tsconfig.json",
		"format:check": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint:check": "eslint .",
		"lint:fix": "eslint --fix .",
		"all": "npm run format:fix && npm run lint:fix && npm run ts:check && npm run build",
		"npm:reinstall": "rm -rf ./node_modules && rm -f ./package-lock.json && npm i && npm i"
	},
	"keywords": [],
